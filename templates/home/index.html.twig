<style>
/* Style for preloader */
#preloader {
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 99999;
    top: 0;
    left: 0;
    background: #000; /* Black background */
    background: -webkit-linear-gradient(to right, #000, #ff0000); /* Black to red gradient */
    background: -o-linear-gradient(to right, #000, #ff0000); /* Black to red gradient */
    background: linear-gradient(to right, #000, #ff0000); /* Black to red gradient */
}

.preload-content {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -35px;
    margin-left: -35px;
    z-index: 100;
}

#loader-load {
    width: 70px;
    height: 70px;
    border-radius: 50%;
    border: 3px solid transparent; /* Adjust border width and style */
    border-top-color: #fff; /* White color for the spinning part */
    animation: inforotate 2400ms linear infinite; /* Rotation animation */
    z-index: 999;
}

#loader-load:before {
    content: "";
    position: absolute;
    top: 7.5px;
    left: 7.5px;
    right: 7.5px;
    bottom: 7.5px;
    border-radius: 50%;
    border: 3px solid transparent;
    border-top-color: #fff; /* White color for the spinning part */
    animation: inforotate 2700ms linear infinite;
}

#loader-load:after {
    content: "";
    position: absolute;
    top: 20px;
    left: 20px;
    right: 20px;
    bottom: 20px;
    border-radius: 50%;
    border: 3px solid transparent;
    border-top-color: #fff; /* White color for the spinning part */
    animation: inforotate 1800ms linear infinite;
}

/* Spin animation */
@keyframes inforotate {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}


</style>





